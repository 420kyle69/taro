{
  "name": "taro-engine",
  "main": "server/ige.js",
  "version": "1.0.0",
  "scripts": {
    "server": "cross-env ENV=standalone node server/ige.js -g ./src",
    "server:prod": "node server/ige.js -g ./src",
    "server:dev": "cross-env ENV=local node server/ige.js -g ./src",
    "server:remote": "cross-env ENV=standalone-remote node server/ige.js -g ./src",
    "build": "cross-env TEST=true node server/ige.js -deploy ./src -to ../be/assets",
    "build:gs": "cross-env TEST=true node server/ige.js -deploy ./src -to ../../be/assets",
    "build:git": "cross-env TEST=true node server/ige.js -deploy ./src -to ./build",
    "debug": "cross-env ENV=standalone node --inspect server/ige.js -g ./src",
    "dev:lint": "eslint . --fix"
  },
  "dependencies": {
    "@clusterws/cws": "^3.0.0",
    "bad-words": "^3.0.4",
    "cookie-parser": "^1.4.4",
    "crash-colliders": "^2.0.2",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lz-string": "^1.4.4",
    "msgpack-lite": "^0.1.26",
    "public-ip": "^4.0.4",
    "q": "^1.5.1",
    "rate-limiter-flexible": "^2.2.2",
    "request": "^2.88.2",
    "sanitizer": "^0.1.3",
    "validator": "^5.7.0",
    "ws": "^7.4.6",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "cross-env": "^5.2.0",
    "eslint": "^7.27.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^4.3.1",
    "node-arguments": "^0.0.1",
    "node-schedule": "^2.0.0",
    "rollbar": "^2.22.0",
    "uglify-js2": "^2.1.11"
  }
}
